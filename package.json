{
  "name": "foodstream-whitelabel-saas",
  "version": "2.0.0",
  "description": "FoodStream White-Label Payment Approval System - Multi-Tenant SAAS",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "dev:client": "cd client && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "build:client": "cd client && npm run build",
    "install:client": "cd client && npm install",
    "test": "jest",
    "migrate": "node database/migrate.js"
  },
  "keywords": [
    "payment",
    "approval",
    "workflow",
    "white-label",
    "saas",
    "multi-tenant"
  ],
  "author": "FoodStream Ltd.",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "twilio": "^4.20.0",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "jest": "^29.7.0",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
